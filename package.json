{
  "name": "npcf-subs-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "lint:src": "eslint src --ext .ts,.tsx",
    "format": "prettier --write \"{app,components,lib,src}/**/*.{ts,tsx,js,jsx,css,md}\"",
    "postinstall": "prisma generate",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "seed": "ts-node --compiler-options {\"module\":\"commonjs\"} seed.ts",
    "dev:sub": "vite --config vite.config.ts",
    "build:sub": "vite build --config vite.config.ts --mode production",
    "preview:sub": "vite preview"
  },
  "dependencies": {
    "@dropbox/sign": "^1.4.3",
    "@hookform/resolvers": "^3.9.0",
    "@prisma/client": "^5.20.0",
    "@types/node": "^22.18.10",
    "@types/react": "^18.3.26",
    "@types/react-dom": "^18.3.0",
    "@vercel/blob": "^0.25.1",
    "autoprefixer": "^10.4.20",
    "jose": "^5.10.0",
    "lucide-react": "^0.462.0",
    "next": "^14.2.0",
    "oidc-client-ts": "^3.3.0",
    "pdf-lib": "^1.17.1",
    "postcss": "^8.4.47",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "react-hook-form": "^7.53.0",
    "react-oidc-context": "^3.3.0",
    "react-router-dom": "^6.28.0",
    "resend": "^4.8.0",
    "tailwindcss": "^3.4.13",
    "typescript": "^5.9.3",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.0",
    "prettier": "^3.6.2",
    "prisma": "^5.20.0",
    "ts-node": "^10.9.2",
    "vite": "^5.4.10"
  }
}
